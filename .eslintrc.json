{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier":["error",{"endOfLine":"auto"}],
        "linebreak-style":"off",
        
    "import/no-unresolved":"off",
    "import/extensions":"off",
    "consistent-return":"off"
    }
}
